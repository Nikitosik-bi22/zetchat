{
  "name": "cunninghares-backend",
  "version": "1.0.0",
  "description": "Cunninghares Messenger Backend",
  "main": "src/server.js",
  "scripts": {
  "start": "node src/server.js",
  "dev": "nodemon src/server.js",
  "db:migrate": "sequelize db:migrate",
  "db:migrate:undo": "sequelize db:migrate:undo",
  "db:migrate:undo:all": "sequelize db:migrate:undo:all",
  "db:init": "psql -U postgres -f database/init.sql",
  "db:reset": "npm run db:drop && npm run db:create && npm run db:init"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.5"
  }
}

